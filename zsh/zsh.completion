#!/usr/bin/env zsh

which memo >/dev/null 2>&1
[[ $? -eq 0 ]] && eval "$(memo --complete)"

#cdの補完関数を利用して保管する
_ch(){
  CDPATH=$HOME
  _cd "cd"
}
compdef _ch ch

_vimrc(){
  _files -W `vimrc --dir` && return 0
}
compdef _vimrc vimrc

_zshrc(){
  _files -W `zshrc --dir` && return 0
}
compdef _zshrc zshrc
