{
  "git url": "https://github.com/peco/peco",
  "Prompt": "[peco] ",
  "InitialFilter": "SmartCase",
  "StickySelection": true,
  "Style": {
    "Query": ["white", "bold"],
    "Basic": ["on_default", "white"],
    "Matched": ["on_default", "yellow"],
    "Selected": ["on_black", "white", "reverse"],
    "SavedSelection": ["on_cyan", "default"]
  },
  "Action": {
    "selectAllAndFinish": [
      "peco.SelectAll",
      "peco.Finish"
    ]
  },
  "Keymap": {
    "M-k": "peco.ToggleSingleKeyJump",
    "C-j": "peco.Finish",
    "C-g": "peco.Cancel",
    "C-v": "peco.ToggleSelectionAndSelectNext",
    "C-y": "peco.SelectAll",
    "C-z": "peco.SelectNone"
  },
  "CustomFilter": {
    "Migemo": {
      "Cmd":  "migemogrep",
      "BufferThreshold": 10000
    }
  }
}

// C-v M-v page up/down
