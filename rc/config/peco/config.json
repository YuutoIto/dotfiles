{
  "git url": "https://github.com/peco/peco",
  "Prompt": "[peco] ",
  "InitialFilter": "SmartCase",
  "StickySelection": true,
  "Use256Color": true,
  "Style": {
    "Basic": ["on_default", "default"],
    "SavedSelection": ["reverse"],
    "Selected": ["underline"],
    "Query": ["45"],
    "Matched": ["45"]
  },
  "Action": {
    "SelectAll_Finish": [
      "peco.SelectAll",
      "peco.Finish"
    ]
  },
  "Keymap": {
    "C-r": "peco.RotateFilter",
    "M-k": "peco.ToggleSingleKeyJump",
    "C-j": "peco.Finish",
    "C-g": "peco.Cancel",
    "C-v": "peco.ToggleSelectionAndSelectNext",
    "C-y": "peco.SelectAll",
    "C-z": "peco.SelectNone"
  },
  "CustomFilter": {
    "Migemo": {
      "Cmd":  "migemogrep",
      "BufferThreshold": 10000
    }
  }
}

// C-v M-v page up/down
