[core]
	editor = vim
[color]
	ui = true
[push]
	default = current
[merge]
	tool = vimdiff
[alias]
  s   = status --short --branch
  st  = status
  d   = diff --color-words
  l   = log --oneline -10
  lo  = log --stat --decorate --find-renames
  co  = commit
  comv = commit -v
  ch  = checkout
  today = log --oneline --since='16 hour ago'
  fix   = commit --amend
  eda   = branch -a
