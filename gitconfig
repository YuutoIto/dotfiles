[user]
	email = uplus.e10@gmail.com
	name = YuutoIto
[core]
	editor = vim
[color]
	ui = true
[push]
	default = current
[merge]
	tool = vimdiff
[alias]
  s   = status --short --branch
  st  = status
  d   = diff
  dw  = diff --color-words
  l   = log --oneline -10
  lo  = log --stat --decorate --find-renames
  ch  = checkout
  co  = commit
  coam  = commit -am
  coa   = commit -a
  cov   = commit -v
  coav  = commit -av
  today = log --oneline --since='16 hour ago'
  fix   = commit --amend -v
  eda   = branch -a
  aliases = !git config --get-regexp '^alias\\.' | sed 's/^alias\\.//g'
