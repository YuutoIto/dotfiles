{
  "git url": "https://github.com/peco/peco",
  "Prompt": "[peco] ",
  "InitialFilter": "SmartCase",
  "StickySelection": true,
  "Style": {
    "Query": ["white", "bold"],
    "Basic": ["on_default", "default"],
    "Selected": ["on_blue", "white"],
    "SavedSelection": ["bold", "on_yellow", "white"],
    "Matched": ["on_cyan", "black"]
  },
  "Keymap": {
    "C-j": "peco.Finish",
    "C-g": "peco.Cancel"
  },
  "CustomFilter": {
    "Migemo": {
      "Cmd":  "migemogrep",
      "BufferThreshold": 10000
    }
  }
}
