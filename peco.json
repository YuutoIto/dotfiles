{
  "git url": "https://github.com/peco/peco",
  "Prompt": "[peco] ",
  "InitialFilter": "SmartCase",
  "StickySelection": true,
  "Style": {
    "Query": ["white", "bold"],
    "Basic": ["on_default", "default"],
    "Selected": ["on_cyan", "black"],
    "SavedSelection": ["bold", "on_cyan", "black"],
    "Matched": ["on_green", "black"]
  },
  "Action": {
    "selectAllAndFinish": [
      "peco.SelectAll",
      "peco.Finish"
    ]
  },
  "Keymap": {
    "C-m": "peco.ToggleSingleKeyJump",
    "C-j": "peco.Finish",
    "C-g": "peco.Cancel",
    "C-y": "peco.SelectAll"
  },
  "CustomFilter": {
    "Migemo": {
      "Cmd":  "migemogrep",
      "BufferThreshold": 10000
    }
  }
}

// C-v M-v page up/down
